<h2>Show Company</h2>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @company.name %>
  </li>

  <li>
    <strong>Created by:</strong>
    <%= @company.created_by_id %>
  </li>

</ul>

<span><%= link "Edit", to: company_path(@conn, :edit, @company) %></span>
<span><%= link "Back", to: company_path(@conn, :index) %></span>
